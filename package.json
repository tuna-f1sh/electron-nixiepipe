{
  "name": "nixie-gui",
  "productName": "Nixie Pipe Controller",
  "version": "1.0.0",
  "description": "Nixie Pipe Electron app",
  "main": "main.js",
  "homepage": "http://www.nixiepipe.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --darwin --overwrite",
    "package-linux": "electron-packager . --linux --overwrite",
    "package-win": "electron-packager . --platform=win32 --arch=x64 --overwrite --prune --electron-version=1.4.15",
    "install-win": "node-pre-gyp install --fallback-to-build --runtime=electron --target=1.4.15 --target_arch=x64 --directory=node_modules/serialport/ --update-binary --dist-url=https://atom.io/download/atom-shell",
    "postinstall": "cd node_modules/serialport && node-pre-gyp rebuild --target=1.4.15 --arch=x64 --dist-url=https://atom.io/download/atom-shell"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tuna-f1sh/electron-nixiepipe.git"
  },
  "author": "John Whittington <node@jbrengineering.co.uk> (http://jbrengineering.co.uk)",
  "license": "GPL-3.0",
  "dependencies": {
    "dotenv": "^4.0.0",
    "electron": "^1.4.15",
    "electron-rebuild": "^1.4.0",
    "forecast": "^0.3.2",
    "jquery": "^3.1.1",
    "nixiepipe": "^1.0.1",
    "node-geocoder": "^3.15.2",
    "os-utils": "0.0.14",
    "serialport": "^4.0.6",
    "simple-color-picker": "^0.1.1",
    "yahoo-finance": "^0.2.16"
  },
  "devDependencies": {
    "electron-packager": "^8.4.0",
    "npm-packager": "0.0.2"
  }
}
